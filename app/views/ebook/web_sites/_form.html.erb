<div class="layui-fluid">
  <div class="layui-row">
    <%= simple_form_for [:ebook, @web_site],remote: true do |f|%>

      <%= hidden_field_tag :back, params[:back]%>

      <%= f.input :name %>
      <%= f.input :code %>
      <%= f.input :url %>
      <%= f.input :remark, label: "说明", input_html: {class: "layui-textarea"}%>

      <%= render form_btn, f: f%>
    <% end %>
  </div>
</div>

<script type="text/javascript">
  $("form").submit(function(){
  }).on("ajax:success", function(r, data, status, xhr){
    alert(data)
  }).on("ajax:complete", function(r){
    debugger
  })
</script>